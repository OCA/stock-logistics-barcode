<?xml version='1.0' encoding='UTF-8'?>
<scenario>
  <id>scanner_scenario_web_location_informations</id>
  <model_id>stock.location</model_id>
  <name>Location informations</name>
  <sequence>200</sequence>
  <parent_id>stock_scanner.scanner_scenario_stock</parent_id>
  <active>True</active>
  <type>scenario</type>
  <Step id="scanner_scenario_web_step_location_informations_unknown_location" name="Unknown location" step_back="False" step_stop="False" step_start="False"/>
  <Step id="scanner_scenario_web_step_location_informations_location_selection" name="Location selection" step_back="False" step_stop="False" step_start="True"/>
  <Step id="scanner_scenario_web_step_location_informations_informations_display" name="Informations display" step_back="False" step_stop="True" step_start="False"/>
  <Transition id="scanner_scenario_web_transition_location_informations_location_selection_ok" name="Location selection OK" sequence="0" tracer="" to_id="scanner_scenario_web_step_location_informations_informations_display" transition_type="keyboard" condition="model.search([('name', '=', message.get('location_name'))])" from_id="scanner_scenario_web_step_location_informations_location_selection"/>
  <Transition id="scanner_scenario_web_transition_location_informations_location_selection_nok" name="Location selection NOK" sequence="0" tracer="" to_id="scanner_scenario_web_step_location_informations_unknown_location" transition_type="keyboard" condition="not model.search([('name', '=', message.get('location_name'))])" from_id="scanner_scenario_web_step_location_informations_location_selection"/>
</scenario>
