<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="label_barcode_quant_report">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t
                    t-call="stock_picking_product_barcode_report_quant.report_label_barcode"
                    t-lang="doc.env.user.lang"
                >
                    <t t-set="label_qty" t-value="1" />
                    <t t-set="product_qty" t-value="doc.quantity" />
                </t>
            </t>
        </t>
    </template>
</odoo>
